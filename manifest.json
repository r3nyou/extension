{
	"manifest_version": 2,
	"name": "Block Extensions",
	"description": "Base Level Extension",
	"version": "1.0",
	"optional_permissions": [
		"tabs",
		"activeTab",
		"notifications",
		"storage"
	],
	"options_page": "options.html",
	"options_ui": {
		"open_in_tab": true,
		"chrome_style": true,
		"page": "options.html"
	},
	"background": {
		"scripts": [
			"js/jquery-3.4.1.min.js",
			"js/Block.js",
			"js/ContextMenus.js",
			"js/Alarm.js"
		]
	},
	"permissions": [
		"storage",
		"webRequest",
		"<all_urls>",
		"http://localhost/",
		"webRequestBlocking",
		"tabs",
		"contextMenus",
		"notifications",
		"alarms",
		"\u003Call_urls>"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}