{
  "name": "Block Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "background": {
    "scripts": [
      "js/Block.js",
      "js/ContextMenus.js",
      "js/Storage.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "tabs",
    "contextMenus",
    "storage"
  ],
  "manifest_version": 2
}
