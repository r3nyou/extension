{
  "manifest_version": 2,
  "name": "Block Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "options_ui": {
    "open_in_tab": true,
    "chrome_style": true,
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "js/jquery-3.4.1.min.js",
      "js/Block.js",
      "js/ContextMenus.js",
      "js/Alarm.js",
      "js/Socket.js",
      "js/socket.io.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "webRequest",
    "<all_urls>",
    "http://localhost/",
    "webRequestBlocking",
    "tabs",
    "contextMenus",
    "notifications",
    "alarms",
    "\u003Call_urls>",
    "activeTab",
    "contextMenus"
  ],
  "icons": { "16": "picture/icon.png" }, 
  "browser_action": {
    "default_icon": "picture/icon.png",
    "default_popup": "popup.html"
  }
}